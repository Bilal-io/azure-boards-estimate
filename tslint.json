{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": {
      "options": [
        "double",
        "jsx-double"
      ]
    },
    "member-access": false,
    "object-literal-sort-keys": false,
    "array-type": false 
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}