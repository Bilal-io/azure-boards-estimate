{
  "manifestVersion": 1,
  "id": "estimate",
  "version": "1.0.19",
  "name": "Estimate",
  "publisher": "ms-devlabs",
  "description": "Planning Poker in Visual Studio Team Services.",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "links": {
    "support": {
      "uri": "mailto:cschleid@microsoft.com"
    }
  },
  "icons": {
    "default": "images/logo.png",
    "branding": "images/logo.png"
  },
  "tags": [
    "plan",
    "agile"
  ],
  "scopes": [
    "vso.work",
    "vso.work_write"
  ],
  "categories": [
    "Plan and track"
  ],
  "content": {
    "details": {
      "path": "details.md"
    }
  },
  "screenshots": [
    {
      "path": "images/screenshot.png"
    }
  ],
  "contributions": [
    {
      "id": "Estimate.Main",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Estimate",
        "order": 99,
        "uri": "dist/web/hub.html"
      }
    },
    {
      "id": "Estimate.WorkItems",
      "type": "ms.vss-web.action",
      "description": "Shows the target properties for menu actions for work items",
      "targets": [
        "ms.vss-work-web.backlog-item-menu"
      ],
      "properties": {
        "text": "Estimate work item(s)",
        "title": "Start estimation session",
        "group": "actions",
        "uri": "dist/web/index.html"
      }
    }
  ],
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "../web",
      "addressable": true
    }
  ]
}